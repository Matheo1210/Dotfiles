{
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.labelFormat": "default",
  "workbench.editor.showTabs": true,
  "editor.renderWhitespace": "none",
  "editor.renderControlCharacters": false,
  "files.trimTrailingWhitespace": true,
  "search.smartCase": true,
  "files.watcherExclude": {
    "**/dist/**": true,
    "**/docker/**": true,
    "**/packages/**/dist": true
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true
  },
  "git.autofetch": true,
  "editor.rulers": [80, 120],
  "namespaceResolver.sortAlphabetically": true,
  "workbench.startupEditor": "welcomePage",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "importSorter.importStringConfiguration.quoteMark": "double",
  "importSorter.sortConfiguration.customOrderingRules.rules": [
    {
      "type": "importMember",
      "regex": "^$",
      "orderLevel": 10,
      "disableSort": true
    },
    {
      "regex": "^@chakra-ui",
      "orderLevel": 20
    },
    {
      "regex": "^[@]",
      "orderLevel": 30
    },
    {
      "regex": "^[.]",
      "orderLevel": 40
    }
  ],
  "importSorter.sortConfiguration.removeUnusedDefaultImports": true,
  "importSorter.sortConfiguration.removeUnusedImports": true,
  "importSorter.generalConfiguration.sortOnBeforeSave": true,
  "debug.showInlineBreakpointCandidates": false,
  "files.insertFinalNewline": true,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "importSorter.importStringConfiguration.maximumNumberOfImportExpressionsPerLine.count": 120,
  "prettier.proseWrap": "always",
  "search.collapseResults": "auto",
  "autoimport.doubleQuotes": true,
  "autoimport.showNotifications": true,
  "debug.allowBreakpointsEverywhere": true,
  "footsteps.highlightColorMaxOpacity": 0.3,
  "vsicons.dontShowNewVersionMessage": true,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.inlineSuggest.enabled": true,
  "editor.autoClosingDelete": "always",
  "redhat.telemetry.enabled": false,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "editor.accessibilitySupport": "off",
  "editor.formatOnSave": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 6,
  "workbench.editor.limit.perEditorGroup": true,
  "smartClicks.rules": {
    "dash": false,
    "html-attr": false,
    "html-element": false,
    "js-arrow-fn": false,
    "js-assign": false,
    "js-colon": false,
    "js-block": false,
    "bracket-pair": false
  },
  "editor.minimap.enabled": false,
  "vscode-json-editor.theme": "dark",
  "i18n-ally.ignoredLocales": ["fedid"],
  "emmet.showAbbreviationSuggestions": false,
  "emmet.showExpandedAbbreviation": "never",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js, ${capture}.typegen.ts",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts, ${capture}.typegen.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock",
    "*.mts": "${capture}.typegen.ts",
    "*.cts": "${capture}.typegen.ts"
  },
  "npm.keybindingsChangedWarningShown": true,
  "[handlebars]": {
    "editor.formatOnSave": false
  },
  "editor.guides.highlightActiveIndentation": false,
  "editor.guides.highlightActiveBracketPair": false,
  "editor.occurrencesHighlight": false,
  "git.mergeEditor": false,
  "editor.snippets.codeActions.enabled": false,
  "editor.snippetSuggestions": "none",
  "terminal.external.osxExec": "warp.app",
  "window.commandCenter": false,
  "git.autoRepositoryDetection": "subFolders",
  "totalTypeScript.hiddenTips": [
    "typing-function-parameters",
    "passing-generics-to-types",
    "union-type",
    "undefined-keyword",
    "basic-types",
    "record-utility-type",
    "generic-slots-in-functions",
    "variable-type-annotation",
    "as-assertion",
    "any-type",
    "array-type",
    "type-alias-declaration",
    "typeof",
    "type-alias-with-generics",
    "ts-object-type",
    "type-alias-with-multiple-generics",
    "keyof",
    "optional-object-property",
    "omit-utility-type",
    "conditional-type",
    "infer",
    "never-keyword",
    "nested-conditional-type",
    "tuple-type",
    "literal-type",
    "in-operator-narrowing",
    "non-null-expression",
    "returntype-utility-type",
    "function-return-type",
    "as-const",
    "parameters-utility-type",
    "null-keyword",
    "interface-declaration"
  ],
  "totalTypeScript.hideAllTips": true,
  "totalTypeScript.hideBasicTips": true,
  "explorer.autoRevealExclude": {
    "**/node_modules": false
  },
  "search.exclude": {
    "**/packages/**/dist": true
  },
  "terminal.integrated.scrollback": 3000,
  "git.suggestSmartCommit": false,
  "excalidraw.theme": "light",
  "typescript.tsserver.log": "off",
  "workbench.sideBar.location": "left",
  "git.openRepositoryInParentFolders": "never",
  "livePreview.portNumber": 5173,
  "livePreview.notifyOnOpenLooseFile": false,
  "git.allowNoVerifyCommit": true,
  "git.confirmNoVerifyCommit": false,
  "console-ninja.featureSet": "Community",
  "diffEditor.ignoreTrimWhitespace": false,
  "svelte.enable-ts-plugin": true,
  "workbench.iconTheme": "vscode-icons",
  "editor.codeActionsOnSave": {},
  "explorer.confirmDelete": false,
  "editor.indentSize": "tabSize"
}
