{
  "workbench.iconTheme": "material-icon-theme",
  "editor.minimap.enabled": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "window.zoomLevel": 1,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.inlineSuggest.enabled": true,
  "editor.insertSpaces": true,
  "editor.tabSize": 4,
  "workbench.startupEditor": "none",
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.inputValidation": "off",
  "git.openRepositoryInParentFolders": "never",
  "extensions.ignoreRecommendations": true,
  "security.workspace.trust.enabled": false,
  "terminal.integrated.cursorBlinking": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit",
    "source.fixAll": "explicit"
  },
  "[mdx]": {
    "editor.wordWrap": "on"
  },
  "[txt]": {
    "editor.wordWrap": "on"
  },
  "[haskell]": {
    "editor.formatOnSave": false
  },
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "yaml": false,
    "jsonc": false
  },
  "html.completion.attributeDefaultValue": "doublequotes",
  "javascript.preferences.quoteStyle": "single",
  "javascript.preferences.jsxAttributeCompletionStyle": "none",
  "typescript.preferences.quoteStyle": "single",
  "typescript.preferences.jsxAttributeCompletionStyle": "none",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "files.autoSaveDelay": 2000,
  "files.enableTrash": true,
  "workbench.editor.limit.value": 6,
  "workbench.editor.limit.enabled": true,
  "indentRainbow.colors": [
    "rgba(255,255,64,0.02)",
    "rgba(127,255,127,0.02)",
    "rgba(255,127,255,0.02)",
    "rgba(79,236,236,0.02)"
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "pnpm-lock.yaml": true
  },
  "explorer.confirmDelete": false,
  "git.confirmSync": false,
  "explorer.confirmDragAndDrop": false,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.accessibilitySupport": "off",
  "terminal.integrated.env.osx": {},
  "editor.language.colorizedBracketPairs": [],
  "editor.indentSize": "tabSize",
  "console-ninja.featureSet": "Community",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "[cpp]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-vscode.cpptools",
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnType": true
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnType": true,
    "editor.tabSize": 2
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnType": true,
    "editor.tabSize": 2
  },
  "[asm-intel-x86-generic]": {
    "editor.defaultFormatter": null,
    "editor.formatOnSave": false,
    "editor.formatOnType": false
  },
  "files.associations": {
    "*.h": "c"
  },
  "npm.keybindingsChangedWarningShown": true,
  "C_Cpp.files.exclude": {
    "**/.vscode": true,
    "**/.vs": true
  },
  "window.titleBarStyle": "custom",
  "haskell.manageHLS": "GHCup",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.suggestSelection": "recentlyUsed",
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "vsintellicode.modify.editor.suggestSelection": "choseToUpdateConfiguration",
  "totalTypeScript.hideAllTips": false,
  "totalTypeScript.hideBasicTips": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "workbench.colorTheme": "Atom One Dark",
  "material-icon-theme.folders.theme": "none",
  "editor.formatOnSaveMode": "modifications",
  "editor.formatOnSave": true
}
