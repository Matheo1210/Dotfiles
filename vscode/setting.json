{
    "workbench.iconTheme": "material-icon-theme",
    "editor.minimap.enabled": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "editor.fontFamily": "CommitMono",
    "window.zoomLevel": 1,
    "terminal.integrated.fontFamily": "CommitMono",
    "editor.formatOnSave": true,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.inlineSuggest.enabled": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit",
      "source.organizeImports": "explicit"
    },
    "[mdx]": {
      "editor.wordWrap": "on"
    },
    "[txt]": {
      "editor.wordWrap": "on"
    },
    "[c]": {
      "editor.formatOnSave": false
    },
    "[haskell]": {
      "editor.formatOnSave": false
    },
    "github.copilot.enable": {
      "*": true,
      "yaml": false,
      "plaintext": false,
      "markdown": false,
      "jsonc": false
    },
    "html.completion.attributeDefaultValue": "doublequotes",
    "javascript.preferences.quoteStyle": "single",
    "javascript.preferences.jsxAttributeCompletionStyle": "none",
    "typescript.preferences.quoteStyle": "single",
    "typescript.preferences.jsxAttributeCompletionStyle": "none",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "files.autoSaveDelay": 2000,
    "files.enableTrash": true,
    "workbench.editor.limit.value": 6,
    "workbench.editor.limit.enabled": true,
    "indentRainbow.colors": [
      "rgba(255,255,64,0.02)",
      "rgba(127,255,127,0.02)",
      "rgba(255,127,255,0.02)",
      "rgba(79,236,236,0.02)"
    ],
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      "**/Thumbs.db": true,
      "pnpm-lock.yaml": true
    },
    "explorer.confirmDelete": false,
    "git.confirmSync": false,
    "[prisma]": {
      "editor.defaultFormatter": "Prisma.prisma"
    },
    "explorer.confirmDragAndDrop": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.accessibilitySupport": "off",
    "terminal.integrated.env.osx": {},
    "editor.language.colorizedBracketPairs": [
    ],
    "editor.indentSize": "tabSize",
    "console-ninja.featureSet": "Community",
    "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.formatOnSave": true,
      "editor.tabSize": 2
    },
    "[javascriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.formatOnSave": true,
      "editor.tabSize": 2
    },
    "[json]": {
      "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.formatOnSave": true,
      "editor.tabSize": 2
    },
    "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.formatOnSave": true,
      "editor.tabSize": 2
    },
    "[python]": {
      "editor.formatOnType": true
    },
    "[cpp]": {
      "editor.tabSize": 4
    },
    "files.associations": {
      "*.h": "c"
    }
  }
