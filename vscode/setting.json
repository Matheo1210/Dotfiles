{
  "git.confirmSync": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontWeight": "400",
  "editor.fontSize": 14,
  "editor.quickSuggestions": {
    "strings": true,
    "comments": false,
    "other": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "liveServer.settings.host": "",
  "editor.formatOnSave": true,
  "workbench.tree.indent": 10,
  "workbench.startupEditor": "none",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "extensions.ignoreRecommendations": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "liveServer.settings.file": "dist/apps/web/index.html",
  "git.ignoreRebaseWarning": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "yaml": false,
    "typescript": false
  },
  "typescript.preferences.includePackageJsonAutoImports": "on",
  "typescript.tsserver.log": "verbose",
  "update.mode": "start",
  "workbench.preferredDarkColorTheme": "Atom One Dark",
  "workbench.tips.enabled": false,
  "workbench.editor.limit.enabled": true,
  "editor.parameterHints.enabled": false,
  "files.exclude": {
    ".vscode": true,
    "**/.git": false
  },
  "[dart]": {
    "editor.formatOnSave": false,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "recentlyUsed",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "debug.terminal.clearBeforeReusing": true,
  "liveServer.settings.donotVerifyTags": true,
  "[python]": {
    "editor.formatOnType": true
  },
  "files.associations": {
    "*.json5": "jsonc",
    "*.h": "c"
  },
  "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "python.terminal.activateEnvironment": false,
  "terminal.integrated.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "application.shellEnvironmentResolutionTimeout": 30,
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.folders.theme": "none",
  "editor.fontLigatures": true,
  "window.autoDetectColorScheme": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabSize": 2,
  "window.commandCenter": false,
  "workbench.activityBar.location": "top",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.wordWrap": "on",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.guides.bracketPairs": true,
  "editor.linkedEditing": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.cts": "${capture}.typegen.ts",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.mts": "${capture}.typegen.ts",
    "*.ts": "${capture}.js, ${capture}.typegen.ts",
    "*.tsx": "${capture}.ts, ${capture}.typegen.ts",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml",
    "tsconfig.json": "tsconfig.*.json"
  },
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "ngClass",
    "style",
    "classNames"
  ],
  "tailwindCSS.emmetCompletions": true,
  "totalTypeScript.hideAllTips": true,
  "totalTypeScript.hideBasicTips": true,
  "git.enableSmartCommit": true,
  "editor.minimap.autohide": true,
  "git.autofetch": true,
  "redhat.telemetry.enabled": true,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "workbench.colorTheme": "Atom One Dark"
}
